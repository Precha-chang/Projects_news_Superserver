{
  "version": 2,
  "builds": [
      {
          "src": "News/wsgi.py",
          "use": "@vercel/python"
      },
      {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
              "distDir": "staticfiles_build"
          }
      }
  ],
  "routes": [
      {
          "src": "/statics/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "News/wsgi.py"
      }
  ]
}